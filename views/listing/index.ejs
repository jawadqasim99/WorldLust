<% layout('/layouts/boilerplate.ejs') -%>
<body>
 <div class="listing row-cols-lg-3 row-cols-md-2 ">
  <% for(let list of listingData){ %>
 
  <a href="/listing/<%=list._id%>" class="listing-link">
  
      <div class="card  ms-3" style="width: 26rem;" >
  
        <div class="card ">
          <img src="<%=list.image%>" class="card-img-top mb-2 " alt="listing ">
          <div class="card-img-overlay " >
           
           
          </div>
        </div>
        <div class="card-body mb-5">
          <h5 style="color: black;"><%= list.title %></h5>
          <p class="card-text" style="color: black;">RS <%= list.price.toLocaleString('en-PK') %> / Night</p>
        </div>
      </div>
    
  </a>
  <% } %> 
  </div>
    
</body>
